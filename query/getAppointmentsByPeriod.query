query {
  getAppointmentsByPeriod(startDateTime: "2020-07-18T10:00:00.000Z", endDateTime: "2020-07-18T20:00:00.000Z") {
    id
    patient {
      id
      firstName
      lastName
    }
    provider {
      id
      firstName
      lastName
    }
    startDateTime
    endDateTime
    location
    room
  }
}
