    provider: "5f153655b575cfb64d5de979",
    startDateTime: "2020-10-14T14:00:00Z",
    endDateTime: "2020-10-14T15:00:00Z",
    location: "Anderson Mill-Austin, TX",
    room: "Rm10",
  }) {
    id
    patient {
      id
      firstName
      lastName
    }
    provider {
      id
      firstName
