mutation addAppointment {
  addAppointment(input: {
    patient: "5f153686b575cfb64d5de97c",
    provider: "5f153655b575cfb64d5de979",
    startDateTime: "2020-10-14T14:00:00Z",
    endDateTime: "2020-10-14T15:00:00Z",
    location: "Anderson Mill-Austin, TX",
    room: "Rm10",
  }) {
    success
    message
    appointment {
      id
      patient 
      provider 
      startDateTime
      endDateTime
      location
      room
    }
  }
}
